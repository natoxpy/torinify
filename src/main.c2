#include <stdio.h>
#ifdef _WIN32
#include <fcntl.h>
#include <io.h>
#include <windows.h>
#endif

int main() {
#ifdef _WIN32

    // Enable binary mode for stdout
    _setmode(_fileno(stdout), _O_U8TEXT);
#endif

    // UTF-8 encoded string
    char str[] = "こんにちは";
    printf("%s\n", str); // Print the UTF-8 string

    return 0;
}